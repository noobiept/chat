{
    "name": "chat",
    "version": "1.2.0",
    "devDependencies": {
        "npm-run-all": "^4.1.3",
        "typescript": "~3.1.3"
    },
    "scripts": {
        "dev": "npm-run-all --parallel dev:*",
        "dev:ts": "tsc -w",
        "dev:swift": "swift run",
        "build": "npm-run-all --parallel build:*",
        "build:ts": "tsc",
        "build:swift": "swift build --configuration release"
    }
}
